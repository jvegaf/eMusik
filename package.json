{
  "name": "emusik",
  "homepage": "^https://github.com/jvegaf/emusik#readme",
  "license": "MIT",
  "description": "The music organizer for classic DJs",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build": "vite build",
    "dev": "vite",
    "emusik": "electron .",
    "emusik:debug": "electron . --enable-logging --devtools",
    "test:typings": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:lint": "eslint . --ext .tsx,.ts,.js",
    "test:lint:fix": "eslint . --ext .tsx,.ts,.js --fix",
    "test:formatting": "prettier --check \"./**/*.{ts,tsx,js}\"",
    "test:formatting:fix": "prettier --write \"./**/*.{ts,tsx,js}\"",
    "test:css": "stylelint \"src/**/*.css\"",
    "package:lmw": "electron-builder -lmw && yarn run package:checksums",
    "package:lw": "electron-builder -lw",
    "package:l": "electron-builder -l",
    "package:w": "electron-builder -w",
    "package:m": "electron-builder -m",
    "package:checksums": "bash scripts/checksum.sh",
    "stylelint": "prettier-eslint --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier-eslint --write \"src/**/*.[ts,tsx,js,jsx]\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvegaf/emusik.git"
  },
  "author": {
    "name": "Jose Vega",
    "email": "josevega234@gmail.com",
    "url": "https://github.com/jvegaf/emusik"
  },
  "devDependencies": {
