{
  "name": "eMusik",
  "version": "0.6.4",
  "description": "Music manager for old-school DJs",
  "main": "./out/main/index.js",
  "author": "Jose Vega",
  "license": "MIT",
  "homepage": "https://github.com/jvegaf/eMusik#readme",
  "repository": "https://github.com/jvegaf/eMusik",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:macsilicon": "electron-vite build && electron-builder --mac --arm64 --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "release": "electron-builder"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.11.3",
    "@mantine/core": "^7.8.0",
    "@mantine/dates": "^7.8.0",
    "@mantine/dropzone": "^7.8.0",
    "@mantine/form": "^7.8.0",
    "@mantine/hooks": "^7.8.0",
    "@mantine/modals": "^7.8.0",
    "@mantine/notifications": "^7.8.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@tabler/icons-react": "^2.47.0",
    "async-g-i-s": "^1.5.2",
    "axios": "^1.6.8",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "electron-context-menu": "^3.6.1",
    "googlethis": "^1.8.0",
    "mantine-react-table": "^2.0.0-alpha.17",
    "music-metadata": "^7.14.0",
    "node-id3": "^0.2.6",
    "react-contexify": "^6.0.0",
    "react-window": "^1.8.10",
    "recursive-readdir": "^2.2.3",
    "styled-components": "^6.1.11",
    "youtube-music-api": "^1.0.6",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^18.19.5",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/recursive-readdir": "^2.2.4",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^28.1.1",
    "electron-builder": "^24.9.1",
    "electron-log": "^5.1.1",
    "electron-vite": "^2.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.14.4",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  }
}
